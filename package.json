  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "mongoose": "^5.3.16",
    "if-env": "^1.0.4",
    "body-parser": "^1.19.0",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "mysql2": "^1.6.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "sequelize": "^5.8.7",
    "sequelize-cli": "^5.4.0"
  }
